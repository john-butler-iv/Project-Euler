\contentsline {subsection}{\numberline {1.1}Lemma: $\forall n \in \mathbb {N}$, $\exists k \in \mathbb {N}: n = 2k$ or $n =2k + 1$}{2}%
\contentsline {subsection}{\numberline {1.2}Theorem: $o$ is odd $\DOTSB \tmspace +\thickmuskip {.2777em}\DOTSB \Leftarrow \joinrel \Rightarrow \tmspace +\thickmuskip {.2777em}\exists n \in \mathbb {N} : o =2n + 1$, and $e$ is even $\DOTSB \tmspace +\thickmuskip {.2777em}\DOTSB \Leftarrow \joinrel \Rightarrow \tmspace +\thickmuskip {.2777em}\exists n \in \mathbb {N} : e = 2n$}{2}%
\contentsline {subsection}{\numberline {1.3}Theorem: An odd number plus an even number equals an odd number, and an odd number plus an odd number equals an even number}{2}%
